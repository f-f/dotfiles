Include local/*

Host *
  ServerAliveInterval 30
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  AddressFamily inet

Host domitian
  Hostname domitian.ferrai.io
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/domitian

Host claudius
  Hostname 192.168.0.105
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/claudius

Host nerva
  Hostname nerva.ferrai.io
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/nerva

Host augustus
  Hostname nilrecurring.duckdns.org
  User fabrizio
  Port 10001
  IdentityFile ~/.ssh/augustus
  ForwardX11 yes
  DynamicForward 5000

Host github.com
  IdentityFile ~/.ssh/github

Host gitlab.com
  IdentityFile ~/.ssh/gitlabcom

